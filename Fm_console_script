
ALTER USER FM DEFAULT TABLESPACE USERS;
ALTER USER FM QUOTA UNLIMITED ON USERS;

CREATE TABLE TB_SEULGAE_API_TEST_TABLE (
   id NUMBER PRIMARY KEY,
   api_name VARCHAR2(100)
);


SELECT *
  FROM TB_SEULGAE_API_TEST_TABLE;

BEGIN
    FOR i IN 1..10000 LOOP
            INSERT INTO TB_SEULGAE_API_TEST_TABLE (id, api_name)
            VALUES (i, 'API_NAME_' || TO_CHAR(i));
        END LOOP;

    COMMIT;
END;

